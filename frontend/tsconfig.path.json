{
  "compilerOptions": {
    "module": "commonjs",
    "paths": {
      "@hooks/*": [
        "src/hooks/*"
      ],
      "@entity/*": [
        "src/entities/*"
      ],
      "@utils/*": [
        "src/utils/*"
      ],
      "@components/*": [
        "src/components/*"
      ],
      "@enums/*": [
        "src/enums/*"
      ]
    },
    "baseUrl": "."
  },
  "exclude": [
    "node_modules"
  ]
}
